{
    "extends": [
        "airbnb", 
        "airbnb/hooks",
        "airbnb-typescript"
    ],
    "parserOptions": {
        "tsconfigRootDir": "./",
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "rules": {
        "@typescript-eslint/default-param-last": "off",
        "constructor-super": [
            "off"
        ],
        "for-direction": [
            "off"
        ],
        "getter-return": [
            "off"
        ],
        "no-async-promise-executor": [
            "off"
        ],
        "no-case-declarations": [
            "off"
        ],
        "no-class-assign": [
            "off"
        ],
        "no-compare-neg-zero": [
            "off"
        ],
        "no-cond-assign": [
            "off"
        ],
        "no-const-assign": [
            "off"
        ],
        "no-constant-condition": [
            "off"
        ],
        "no-control-regex": [
            "off"
        ],
        "no-debugger": [
            "off"
        ],
        "no-delete-var": [
            "off"
        ],
        "no-dupe-args": [
            "off"
        ],
        "no-dupe-class-members": [
            "off"
        ],
        "no-dupe-else-if": [
            "off"
        ],
        "no-dupe-keys": [
            "off"
        ],
        "no-duplicate-case": [
            "off"
        ],
        "no-empty": [
            "off"
        ],
        "no-empty-character-class": [
            "off"
        ],
        "no-empty-pattern": [
            "off"
        ],
        "no-ex-assign": [
            "off"
        ],
        "no-extra-boolean-cast": [
            "off"
        ],
        "no-extra-semi": [
            "off"
        ],
        "no-fallthrough": [
            "off"
        ],
        "no-func-assign": [
            "off"
        ],
        "no-global-assign": [
            "off"
        ],
        "no-import-assign": [
            "off"
        ],
        "no-inner-declarations": [
            "off"
        ],
        "no-invalid-regexp": [
            "off"
        ],
        "no-irregular-whitespace": [
            "off"
        ],
        "no-loss-of-precision": [
            "off"
        ],
        "no-misleading-character-class": [
            "off"
        ],
        "no-mixed-spaces-and-tabs": [
            "off"
        ],
        "no-new-symbol": [
            "off"
        ],
        "no-nonoctal-decimal-escape": [
            "off"
        ],
        "no-obj-calls": [
            "off"
        ],
        "no-octal": [
            "off"
        ],
        "no-prototype-builtins": [
            "off"
        ],
        "no-redeclare": [
            "off"
        ],
        "no-regex-spaces": [
            "off"
        ],
        "no-self-assign": [
            "off"
        ],
        "no-setter-return": [
            "off"
        ],
        "no-shadow-restricted-names": [
            "off"
        ],
        "no-sparse-arrays": [
            "off"
        ],
        "no-this-before-super": [
            "off"
        ],
        "no-undef": [
            "off"
        ],
        "no-unexpected-multiline": [
            "off"
        ],
        "no-unreachable": [
            "off"
        ],
        "no-unsafe-finally": [
            "off"
        ],
        "no-unsafe-negation": [
            "off"
        ],
        "no-unsafe-optional-chaining": [
            "off"
        ],
        "no-unused-labels": [
            "off"
        ],
        "no-unused-vars": [
            "off"
        ],
        "no-useless-backreference": [
            "off"
        ],
        "no-useless-catch": [
            "off"
        ],
        "no-useless-escape": [
            "off"
        ],
        "no-with": [
            "off"
        ],
        "require-yield": [
            "off"
        ],
        "use-isnan": [
            "off"
        ],
        "valid-typeof": [
            "off"
        ]
    }
}